<div class="form-wrapper">
  <%= form_with model: [ @team, @team.students.build ] do |f| %>

    <div class="field">
      <%= f.label :first_name %><br />
      <%= f.text_field :first_name, class: "field-text" %>
    </div>

    <div class="field">
      <%= f.label :last_name %><br />
      <%= f.text_field :last_name, class: "field-text" %>
    </div>

    <div class="field">
      <%= f.label :contact %><br />
      <%= f.text_field :contact, class: "field-text" %>
    </div>

    <div class="actions">
      <%= f.submit "Add Student", class: "form-button", data: {test: "create-student"} %>
    </div>
  <% end %>
</div>