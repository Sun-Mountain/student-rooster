<div id="dashboard">
  <h1>Dashboard</h1>

  <div class="dash-teams-list">
    <h2>Your Teams</h2>
    <% if @teams.count == 0 && @memberships.count == 0%>
      <p>You are not a member of any teams.</p>
      <p>Create your first team <%= link_to 'here', new_team_path %>.</p>
    <% end %>

  </div>
</div>