<nav id='main-navigation'>
  <ul>
    <li>
      <%= link_to 'Home', root_path, :class => current_class?(root_path) %>
    </li>
    <% if user_signed_in? %>
      <li>
        <%= link_to 'Logout', destroy_user_session_path, data: { turbo_method: :delete } %>
      </li>
    <% else %>
      <li>
        <%= link_to 'Log In', new_user_session_path, :class => current_class?(new_user_session_path) %>
      </li>
      <li>
        <%= link_to 'Register', new_user_registration_path, :class => current_class?(new_user_registration_path) %>
      </li>
    <% end %>
  </ul>
</nav>