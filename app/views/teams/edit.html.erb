<div class="form-container">
  <div class="form-wrapper">
    <%= form_for @team, url: user_team_path do |form| %>
      <div class="field">
        <%= form.label :name %><br>
        <%= form.text_field :name, class: "field-text" %>
      </div>

      <div class="field">
        <%= form.submit "Update Team", class: "form-button"  %>
      </div>
    <% end %>
  </div>
</div>